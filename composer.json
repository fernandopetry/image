{
    "name": "fernandopetry/image",
    "description": "Manage images",
    "keywords": ["fernandopetry", "php", "image"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Petry\\Image\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Petry\\Image\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
